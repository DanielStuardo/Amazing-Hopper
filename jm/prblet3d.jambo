  Clr all marks
  z=0
  [3,3,1:2:3] z=-5
  Printnl("Z=\n",z)
  Clr all marks
  z=0
  [3,1:2:3,1:3] z=-5
  Printnl("Z=\n",z)
  
  Dim (6) as ones 'x'
  Clr all marks
  t=z
  Clear 'z'
  z=x
  Printnl("Z=\n",z)
  
  Clear 'x'
  Dim (9,9) as int rand '10, x'
  [3:7,1:2:Cols(x)] z = x  // cuando fuente es array, la marca será de él. Z será reemplazado
  
  Printnl("X=\n",x,"\nZ=\n",z, "\nTipo Z=", Type(z))
  
  w=10, h=w
  Printnl("W=",w," h=",h)
  
  Clear 'z', Clr all marks
  Dim (5,5,5) as zeros 'z'
  Printnl("Z=\n",z)
  [1:2:Rows(z), 1:2:Cols(z), 1:2:Pags(z)] z = "HOLA"
  Printnl("Z=\n",z)
End

