Main
  Dim (3,3,3) as zeros 'z'
  Printnl("Z=\n",z)

  [3,1,1] z=100
  Printnl("Z=\n",z)

  [1,3,1] z=100
  Printnl("Z=\n",z)

  [1,1,3] z=100
