
#include <jambo.h>


Main
   Init zero 't1, t2'
   
   Tic(t1)
   Gosub while ( i=0, Less(i,1000000), Suma todo )
   Toc(t1,t2), Printnl("I=",i,"\nTIME = ",t2)

   Tic(t1)
   i=0
   Loop while (Less(i,1000000))
       Gosub 'Suma  todo'
   Back
   Toc(t1,t2), Printnl("I=",i,"\nTIME = ",t2)

   Tic(t1)
   i=0
   Loop while (Less(i,1000000))
       ++i
   Back
   Toc(t1,t2), Printnl("I=",i,"\nTIME = ",t2)
   
   Tic(t1)
   i=0
   Gosub times (1000000, Suma todo )
   Toc(t1,t2), Printnl("I=",i,"\nTIME = ",t2)
   
   Gosub if ( Not positive 'i', Suma todo )
   Printnl("I=",i)
